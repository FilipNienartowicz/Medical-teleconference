@model IEnumerable<Medical_teleconference.Models.Photo>

<div class="col-lg-3" id="listDiv">
    <p>List of Photos:</p>
    @if (Model != null)
    {
        <ul id="imageList">
            @foreach (var item in Model)
            {
            <li>
                <a>
                    @Ajax.ActionLink(item.PhotoId.ToString(), "Show", new { id = item.PhotoId }, new AjaxOptions()
           {
               HttpMethod = "Get",
               UpdateTargetId = "imageToTransform",

           }, new { data = item.PhotoId })
                </a>
            </li>
            }
        </ul>
    }
    else
    {
        <h2>Nothing to show here</h2>
    }
</div>
